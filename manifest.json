{
  "name": "eagle",
  "version": "1.0",
  "manifest_version": 2,
  "description": "a page performances toolkit for chrome extension",
  "icons": {
    "128": "./public/icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["bg.bundle.js"]
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "js": ["content.bundle.js"]
    }
  ],
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "scripting",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "./public/icon.png",
    "default_popup": "popup.html"
  },
  "default_locale": "zh_CN",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "A page Performances Tool for Google Chrome Extension"
    }
  }
}
